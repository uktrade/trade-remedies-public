web: if [ "$DEBUG" == "True" ]; then python -m pip install -U -r requirements/dev.txt --no-warn-script-location; else python -m pip install -U -r requirements/prod.txt --no-warn-script-location; fi && cd trade_remedies_public && python ./manage.py migrate && python ./manage.py compilescss && ./manage.py collectstatic --noinput && gunicorn config.wsgi --bind 0.0.0.0:8080 --capture-output --config config/gunicorn.py

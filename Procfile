web: if [ "$DEBUG" == "True" ]; then pip install -U -r requirements/dev.txt; else pip install -U -r requirements/prod.txt; fi && cd trade_remedies_public && python ./manage.py migrate && python ./manage.py compilescss && ./manage.py collectstatic --noinput && gunicorn config.wsgi --bind 0.0.0.0:8080 --capture-output --config config/gunicorn.py
